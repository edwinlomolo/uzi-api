# User
type User {
  id: UUID!
  first_name: String!
  last_name: String!
  phone: String!
  created_at: Time
  updated_at: Time
}

type Session {
  id: UUID!
  ip: String!
  token: String!
  user_id: UUID!
  expires: Time!
  created_at: Time
  updated_at: Time
}

type Query {
  hello: String!
}

schema {
  query: Query
}

scalar Time
scalar UUID
